// Program: Check year

#include <iostream>
using namespace std;

int main() {
    int year;

    do {
        cout << "Enter year: ";
        cin >> year;

        if (year > 2018) {
            cout << "Year must be 2018 or earlier" << endl;
        }
    } while (year > 2018);

    cout << "You entered: " << year << endl;

    return 0;
}
